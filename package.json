{
  "name": "tesla-ah-frontend",
  "version": "1.0.0",
  "description": "Reactjs frontend implementation for AH",
  "main": "index.js",
  "repository": "git@github.com:andela/tesla-ah-frontend",
  "author": "Tesla Team",
  "license": "MIT",
  "scripts": {
    "start": "serve -s ./build",
    "test": "jest --coverage -u",
    "test:watch": "jest --watch --coverage",
    "dev": "webpack-dev-server --mode development --open",
    "build": "webpack --mode production",
    "lint": "eslint src -c .eslintrc --ext js,jsx",
    "lint:tests": "eslint __tests__ -c .eslintrc --ext js"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-loader": "^8.0.6",
    "css-loader": "^3.1.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-to-json": "^3.3.5",
    "error-overlay-webpack-plugin": "^0.4.0",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.8.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moxios": "^0.4.0",
    "prettier": "^1.18.2",
    "redux-mock-store": "^1.5.3",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.7.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "async": "^3.1.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-favicon": "^2.0.1",
    "joi-browser": "^13.4.0",
    "lodash": "^4.17.15",
    "moxios": "^0.4.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "popper.js": "^1.15.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "react-tiny-fab": "^3.0.1",
    "react-toastify": "^5.3.2",
    "redux": "^4.0.4",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "serve": "^11.1.0",
    "webpack-bundle-analyzer": "^3.4.1"
  },
  "jest": {
    "verbose": true,
    "setupFilesAfterEnv": [
      "./__tests__/setup/setupEnzyme.js"
    ],
    "testPathIgnorePatterns": [
      "./__tests__/setup/",
      "./__tests__/mockData/"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "<rootDir>/__mocks__/styleMock.js"
    }
  }
}
